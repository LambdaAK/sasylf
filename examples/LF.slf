// Lemma 2.10 (3)
theorem subst-r-rh-compose:
    assumes Γ
    forall α0
    forall α2
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} r α2 R1[x0][x] = R[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} r α0 R1[x0][x] = M1'[x] : α
    exists Γ ⊢ {M0/x0} r α0 R[x0] = M' : α and Γ ⊢ {M2'/x} m α2 M1'[x] = M'
    use induction on { α0, α2 }, s2
    do case analysis on s2:
        case rule
            v: R[x0] : A1[x0] ∈ (Γ, x0 : A0)
            ---------------------------------------------- Subst-R-Var
            _: Γ, x0 : A0 ⊢ ({M2[x0]/x} r α2 R[x0]) = R[x0]
            where R1[x0][x] := R[x0]
        is
            proof by case analysis on v:
                case rule
                    ------------------------------------- var
                    _: x : A2 ∈ (Γ', x : A2, x0 : A3[x])
                    where A1[x0] := A2
                is
                    proof by contradiction on s3
                end case

                case rule
                    ---------------------- var
                    _: x : A0 ∈ (Γ, x : A0)
                    where R[x0] := x0 and A1[x0] := A0
                is
                    use inversion of rule Subst-RH-Var on s3
                    where M1'[x] := M0 and α := α0
                    s4: Γ ⊢ {M0/x0} r α0 x0 = M0 : α0 by rule Subst-RH-Var 
                    s5: Γ ⊢ {M2'/x} m α2 M0 = M0 by theorem nop-subst-m on M2', α2, M0
                    proof by s4, s5
                end case
            end case analysis
        end case
    end case analysis
    proof by case analysis on s3:
        case rule
            ------------------------------------ Subst-RH-Var
            _: Γ, x : A ⊢ ({M0/x0} r α0 x0) = M0 : α0
            where R1[x0][x] := x0 and α := α0 and M1'[x] := M0
        is
            proof by contradiction on s2
        end case

        case rule
            s31: Γ, x : A ⊢ ({M0/x0}    r α0 R11[x0][x]) = (λ x' • M111[x][x']) : (α1 → α)
            s32: Γ, x : A ⊢ ({M0/x0}    m α0 M12[x0][x]) = M4[x]
            s33: Γ, x : A ⊢ ({M4[x]/x'} m α1 M111[x][x']) = M1'[x]
            ------------------------------------------- Subst-RH-App
            _: Γ, x : A ⊢ ({M0/x0} r α0 (R11[x0][x] M12[x0][x])) = M1'[x] : α
            where R1[x0][x] := R11[x0][x] M12[x0][x] 
        is
            s21: Γ, x0 : A0 ⊢ ({M2[x0]/x} r α2 R11[x0][x]) = R21[x0] and
            s22: Γ, x0 : A0 ⊢ ({M2[x0]/x} m α2 M12[x0][x]) = M22[x0] by inversion on s2
            where R[x0] := R21[x0] M22[x0]
            s41: Γ ⊢ {M0/x0} r α0 R21[x0] = M21' : α1 → α and
            s51: Γ ⊢ {M2'/x} m α2 (λ x' • M111[x][x']) = M21' by induction hypothesis on α0, α2, s1, s21, s31
            s23: Γ, x':0 ⊢ {M2'/x} m α2 M111[x][x'] = M211[x'] by inversion on s51
            where M21' := λ x' • M211[x']
            sx22: α2 ≤ α2 by rule sub-eq
            s42: Γ ⊢ {M0/x0} m α0 M22[x0] = M22' and
            s52: Γ ⊢ {M2'/x} m α2 M4[x] = M22' by theorem subst-m-composeI on α0, α2, sx22, s1, s22, s32
            sbx: α1 → α ≤ α0 by theorem head-substitution-size on s31
            sx11: α1 ≤ α1 by rule sub-eq
            sx11': α1 ≤ α1 → α by rule sub-arrow1 on sx11
            s5: Γ ⊢ {M2'/x} m α2 M1'[x] = M' and
            s43: Γ ⊢ {M22'/x'} m α1 M211[x'] = M' by case analysis on sbx:
                case rule
                    --------------------- sub-eq
                    _: (α1 → α) ≤ (α1 → α)
                    where α0 := (α1 → α)
                is
                    proof by theorem subst-m-composeI on α2, α1, sx11, s52, s33, s23
                end case

                case rule
                    sx103: (α1 → α) ≤ α3
                    ------------------- sub-arrow1
                    _: (α1 → α) ≤ (α3 → α4)
                    where α0 := (α3 → α4)
                is
                    sx13: α1 ≤ α3 by lemma subexpression-transitive on sx11', sx103
                    proof by theorem subst-m-composeI on α2, α3, sx13, s52, s33, s23
                end case

                case rule
                    sx104: (α1 → α) ≤ α4
                    ------------------- sub-arrow2
                    _: (α1 → α) ≤ (α3 → α4)
                    where α0 := (α3 → α4)
                is
                    sx14: α1 ≤ α4 by lemma subexpression-transitive on sx11', sx104
                    proof by theorem subst-m-composeI on α2, α4, sx14, s52, s33, s23
                end case

            end case analysis
            s4: Γ ⊢ {M0/x0} r α0 R21[x0] M22[x0] = M' : α by rule Subst-RH-App on s41, s42, s43
            proof by s4, s5
        end case

    end case analysis
end theorem
 
// Lemma 2.10 (1) M 
// without induction arguments
theorem subst-m-compose:
    assumes Γ
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} m α2 M1[x0][x] = M[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} m α0 M1[x0][x] = M1'[x]
    exists Γ ⊢ {M0/x0} m α0 M[x0] = M' and Γ ⊢ {M2'/x} m α2 M1'[x] = M'
    sx: α2 ≤ α2 by rule sub-eq
    proof by theorem subst-m-composeI on α0, α2, sx, s1, s2, s3
end theorem

// Lemma 2.10 (1) R
// without induction arguments
theorem subst-r-compose:
    assumes Γ
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} r α2 R1[x0][x] = R[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} r α0 R1[x0][x] = R1'[x]
    exists Γ ⊢ {M0/x0} r α0 R[x0] = R' 
    and    Γ ⊢ {M2'/x} r α2 R1'[x] = R'
    proof by theorem subst-r-composeI on α0, α2, s1, s2, s3
end theorem

// Lemma 2.10 (1) P
theorem subst-p-compose:
    assumes Γ
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} p α2 P1[x0][x] = P[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} p α0 P1[x0][x] = P1'[x]
    exists Γ ⊢ {M0/x0} p α0 P[x0] = P' 
    and    Γ ⊢ {M2'/x} p α2 P1'[x] = P'
    proof by induction on s2:
        case rule
            ------------------------ Subst-P-Fam
            _: Γ, x0 : A0 ⊢ ({M2[x0]/x} p α2 a) = a
            where P1[x0][x] := a and P[x0] := a
        is
            use inversion on s3
            where P1'[x] := a
            s4: Γ ⊢ {M0/x0} p α0 a = a by rule Subst-P-Fam 
            s5: Γ ⊢ {M2'/x} p α2 a = a by rule Subst-P-Fam
            proof by s4, s5
        end case

        case rule
            s21: Γ, x0 : A0 ⊢ ({M2[x0]/x} p α2 P11[x0][x]) = P12[x0]
            s22: Γ, x0 : A0 ⊢ ({M2[x0]/x} m α2 M11[x0][x]) = M12[x0]
            ---------------------------------------------------------------- Subst-P-App
            _:   Γ, x0 : A0 ⊢ ({M2[x0]/x} p α2 (P11[x0][x] M11[x0][x])) = (P12[x0] M12[x0])
            where P1[x0][x] := P11[x0][x] M11[x0][x] and P[x0] := P12[x0] M12[x0]
        is
            s31: Γ, x:A ⊢ {M0/x0} p α0 P11[x0][x] = P13[x] and
            s32: Γ, x:A ⊢ {M0/x0} m α0 M11[x0][x] = M13[x] by inversion on s3
            where P1'[x] := P13[x] M13[x]
            s41: Γ ⊢ {M0/x0} p α0 P12[x0] = P16 and 
            s51: Γ ⊢ {M2'/x} p α2 P13[x] = P16 by induction hypothesis on s1, s21, s31
            s42: Γ ⊢ {M0/x0} m α0 M12[x0] = M16 and 
            s52: Γ ⊢ {M2'/x} m α2 M13[x] = M16 by theorem subst-m-compose on s1, s22, s32
            s4: Γ ⊢ {M0/x0} p α0 P12[x0] M12[x0] = P16 M16 by rule Subst-P-App on s41, s42 
            s5: Γ ⊢ {M2'/x} p α2 P13[x] M13[x] = P16 M16 by rule Subst-P-App on s51, s52
            proof by s4, s5
        end case

    end induction
end theorem
 
// Lemma 2.10 (1) A (with extra induction argument)
theorem subst-a-composeI:
    assumes Γ
    forall A1[x0][x] assumes Γ, x:A, x0:A0
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} α2 A1[x0][x] = A10[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} α0 A1[x0][x] = A1'[x]
    exists Γ ⊢ {M0/x0} α0 A10[x0] = A' 
    and    Γ ⊢ {M2'/x} α2 A1'[x] = A'
    use induction on (A1[x0][x] assumes Γ, x:A, x0:A0)
    proof by case analysis on s2:
        case rule
            s2': Γ, x0 : A0 ⊢ ({M2[x0]/x} p α2 P1[x0][x]) = P10[x0]
            --------------------------------------------------- Subst-A-P
            _: Γ, x0 : A0 ⊢ ({M2[x0]/x} α2 P1[x0][x]) = P10[x0]
            where A1[x0][x] := P1[x0][x] and A10[x0] := P10[x0]
        is
            s3': Γ, x:A ⊢ {M0/x0} p α0 P1[x0][x] = P1'[x] by inversion on s3
            where A1'[x] := P1'[x]
            s4': Γ ⊢ {M0/x0} p α0 P10[x0] = P' and
            s5': Γ ⊢ {M2'/x} p α2 P1'[x] = P' by theorem subst-p-compose on s1, s2', s3'
            s4: Γ ⊢ {M0/x0} α0 P10[x0] = P' by rule Subst-A-P on s4'
            s5: Γ ⊢ {M2'/x} α2 P1'[x] = P' by rule Subst-A-P on s5'
            proof by s4, s5
        end case

        case rule
            s21: Γ, x0 : A0 ⊢ ({M2[x0]/x} α2 A11[x0][x]) = A21[x0]
            s22: Γ, x0 : A0, x' : A21[x0] ⊢ ({M2[x0]/x} α2 A12[x0][x][x']) = A22[x0][x']
            --------------------------------------------------------------- Subst-A-Pi
            _:   Γ, x0 : A0 ⊢ ({M2[x0]/x} α2 (Π x' : A11[x0][x] • A12[x0][x][x'])) = (Π x' : A21[x0] • A22[x0][x'])
            where A1[x0][x] := Π x' : A11[x0][x] • A12[x0][x][x']
             and A10[x0] := Π x' : A21[x0] • A22[x0][x']
        is
            proof by case analysis on s3:
                case rule
                    s31: Γ, x:A ⊢ {M0/x0} α0 A11[x0][x] = A31[x]
                    s32: Γ, x:A, x':A31[x] ⊢ {M0/x0} α0 A12[x0][x][x'] = A32[x][x']
                    -------------------------------------------------------------- Subst-A-Pi
                    _: Γ, x:A ⊢ {M0/x0} α0 Π x' : A11[x0][x] • A12[x0][x][x'] = Π x' : A31[x] • A32[x][x']
                    where A1'[x] := Π x' : A31[x] • A32[x][x']
                is
                    s41: Γ ⊢ {M0/x0} α0 A21[x0] = A11' and 
                    s51: Γ ⊢ {M2'/x} α2 A31[x] = A11' by induction hypothesis on (A11[x0][x] assumes Γ, x:A, x0:A0), s1, s21, s31
                    s23: Γ, x0 : A0, x' : A11' ⊢ ({M2[x0]/x} α2 A12[x0][x][x']) = A22[x0][x'] by lemma subst-ignores-variable-type on s22, A11'
                    s33: Γ, x:A, x':A11' ⊢ {M0/x0} α0 A12[x0][x][x'] = A32[x][x'] by lemma subst-ignores-variable-type on s32, A11'
                    s24: Γ, x':A11', x0:A0 ⊢ ({M2[x0]/x} α2 A12[x0][x][x']) = A22[x0][x'] by exchange on s23
                    s34: Γ, x':A11', x : A ⊢ {M0/x0} α0 A12[x0][x][x'] = A32[x][x'] by exchange on s33
                    s14: Γ, x':A11' ⊢ {M0/x0} m α0 M2[x0] = M2' by weakening on s1
                    s42: Γ, x':A11' ⊢ {M0/x0} α0 A22[x0][x'] = A12'[x'] and 
                    s52: Γ, x':A11' ⊢ {M2'/x} α2 A32[x][x'] = A12'[x'] by induction hypothesis on (A12[x0][x][x'] assumes Γ, x':A11', x:A, x0:A0), s14, s24, s34
                    s4: Γ ⊢ {M0/x0} α0 (Π x' : A21[x0] • A22[x0][x']) = (Π x' : A11' • A12'[x']) by rule Subst-A-Pi on s41, s42
                    s5: Γ ⊢ {M2'/x} α2 (Π x' : A31[x]  • A32[x] [x']) = (Π x' : A11' • A12'[x']) by rule Subst-A-Pi on s51, s52
                    proof by s4, s5
                end case
            end case analysis
        end case
    end case analysis
end theorem

// Lemma 2.10 (1) A 
theorem subst-a-compose:
    assumes Γ
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} α2 A1[x0][x] = A10[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} α0 A1[x0][x] = A1'[x]
    exists Γ ⊢ {M0/x0} α0 A10[x0] = A' 
    and    Γ ⊢ {M2'/x} α2 A1'[x] = A'
    proof by theorem subst-a-composeI on (A1[x0][x] assumes Γ, x:A, x0:A0), s1, s2, s3
end theorem

// Lemma 2.10 (1) K (with extra induction argument)
theorem subst-k-composeI:
    assumes Γ
    forall K1[x0][x] assumes Γ, x:A, x0:A0
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} α2 K1[x0][x] = K[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} α0 K1[x0][x] = K1'[x]
    exists Γ ⊢ {M0/x0} α0 K[x0] = K' 
    and    Γ ⊢ {M2'/x} α2 K1'[x] = K'
    use induction on (K1[x0][x] assumes Γ, x:A, x0:A0)
    proof by case analysis on s2:
        case rule
            ----------------------------------------- Subst-K-Type
            _: Γ, x0 : A0 ⊢ {M2[x0]/x} α2 type = type
            where K1[x0][x] := type and K[x0] := type
        is
            use inversion on s3 where K1'[x] := type
            s4: Γ ⊢ {M0/x0} α0 type = type by rule Subst-K-Type
            s5: Γ ⊢ {M2'/x} α2 type = type by rule Subst-K-Type
            proof by s4, s5
        end case


        case rule
            s21: Γ, x0 : A0 ⊢ ({M2[x0]/x} α2 A11[x0][x]) = A21[x0]
            s22: Γ, x0 : A0, x' : A21[x0] ⊢ ({M2[x0]/x} α2 K12[x0][x][x']) = K22[x0][x']
            --------------------------------------------------------------- Subst-K-Pi
            _:   Γ, x0 : A0 ⊢ ({M2[x0]/x} α2 (Π x' : A11[x0][x] • K12[x0][x][x'])) = (Π x' : A21[x0] • K22[x0][x'])
            where K1[x0][x] := Π x' : A11[x0][x] • K12[x0][x][x']
             and K[x0] := Π x' : A21[x0] • K22[x0][x']
        is
            proof by case analysis on s3:
                case rule
                    s31: Γ, x:A ⊢ {M0/x0} α0 A11[x0][x] = A31[x]
                    s32: Γ, x:A, x':A31[x] ⊢ {M0/x0} α0 K12[x0][x][x'] = K32[x][x']
                    -------------------------------------------------------------- Subst-K-Pi
                    _: Γ, x:A ⊢ {M0/x0} α0 Π x' : A11[x0][x] • K12[x0][x][x'] = Π x' : A31[x] • K32[x][x']
                    where K1'[x] := Π x' : A31[x] • K32[x][x']
                is
                    s41: Γ ⊢ {M0/x0} α0 A21[x0] = A11' and 
                    s51: Γ ⊢ {M2'/x} α2 A31[x] = A11' by theorem subst-a-compose on s1, s21, s31
                    s23: Γ, x0 : A0, x' : A11' ⊢ ({M2[x0]/x} α2 K12[x0][x][x']) = K22[x0][x'] by lemma subst-ignores-variable-type on s22, A11'
                    s33: Γ, x:A, x':A11' ⊢ {M0/x0} α0 K12[x0][x][x'] = K32[x][x'] by lemma subst-ignores-variable-type on s32, A11'
                    s24: Γ, x':A11', x0:A0 ⊢ ({M2[x0]/x} α2 K12[x0][x][x']) = K22[x0][x'] by exchange on s23
                    s34: Γ, x':A11', x : A ⊢ {M0/x0} α0 K12[x0][x][x'] = K32[x][x'] by exchange on s33
                    s14: Γ, x':A11' ⊢ {M0/x0} m α0 M2[x0] = M2' by weakening on s1
                    s42: Γ, x':A11' ⊢ {M0/x0} α0 K22[x0][x'] = K12'[x'] and 
                    s52: Γ, x':A11' ⊢ {M2'/x} α2 K32[x][x'] = K12'[x'] by induction hypothesis on (K12[x0][x][x'] assumes Γ, x':A11', x:A, x0:A0), s14, s24, s34
                    s4: Γ ⊢ {M0/x0} α0 (Π x' : A21[x0] • K22[x0][x']) = (Π x' : A11' • K12'[x']) by rule Subst-K-Pi on s41, s42
                    s5: Γ ⊢ {M2'/x} α2 (Π x' : A31[x]  • K32[x] [x']) = (Π x' : A11' • K12'[x']) by rule Subst-K-Pi on s51, s52
                    proof by s4, s5
                end case
            end case analysis
        end case
    end case analysis
end theorem

// Lemma 2.10 (1) A 
theorem subst-k-compose:
    assumes Γ
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} α2 K1[x0][x] = K[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} α0 K1[x0][x] = K1'[x]
    exists Γ ⊢ {M0/x0} α0 K[x0] = K' 
    and    Γ ⊢ {M2'/x} α2 K1'[x] = K'
    proof by theorem subst-k-composeI on (K1[x0][x] assumes Γ, x:A, x0:A0), s1, s2, s3
end theorem


/// Substitution theorems

// We follow the proof sketch of H&L but because we can't
// reason about things in the middle of the context, we assume Γ_R is empty
// and in induction, we perform substitution so that we can "exchange" the
// relevant context item to the front.  Unfortunately this isn't sound:
// see the "unproved" step below:

theorem subst-preserves-canon-term:
    assumes Γ
    forall α0
    forall α1
    forall sx: α0 ≤ α1
    forall M[x0] assumes Γ, x0:A0
    forall ct1: Γ, x0:A0 ⊢Σ M[x0] ⇐ A[x0]
    forall arg: Γ ⊢Σ M0 ⇐ A0
    forall sim: Γ ⊢ (A0)⁻ = α0 
    forall sbA: Γ ⊢ {M0/x0} α0 A[x0] = A'
    exists Γ ⊢ {M0/x0} m α0 M[x0] = M'
    and Γ ⊢Σ M' ⇐ A'
    use induction on α1, (M[x0] assumes Γ, x0:A0)
    do case analysis on sx:
        case rule
            sx2: α0 ≤ α2
            ------------------- sub-arrow1
            _: α0 ≤ (α2 → α3)
            where α1 := (α2 → α3)
        is
            proof by induction hypothesis on α0, α2, sx2, (M[x0] assumes Γ, x0:A0), ct1, arg, sim, sbA
        end case

        case rule
            sx3: α0 ≤ α3
            ------------------- sub-arrow2
            _: α0 ≤ (α2 → α3)
            where α1 := (α2 → α3)
        is
            proof by induction hypothesis on α0, α3, sx3, (M[x0] assumes Γ, x0:A0), ct1, arg, sim, sbA
        end case
    end case analysis
    use inversion on sx where α1 := α0
    proof by case analysis on ct1:
        case rule
            at1: Γ, x0 : A0 ⊢Σ R[x0] ⇒ P[x0]
            ----------------------------- Canon-Term-Atom
            _:   Γ, x0 : A0 ⊢Σ R[x0] ⇐ P[x0]
            where M[x0] := R[x0] and A[x0] := P[x0]
        is
            sbP: Γ ⊢ {M0/x0} p α0 P[x0] = P' by inversion on sbA
            where A' := P'
            sbA': Γ ⊢ {M0/x0} α0 P[x0] = A'' and
            rest: ((Γ ⊢ {M0/x0} r α0 R[x0] = R'' and Γ ⊢Σ R'' ⇒ A'') or
                   (Γ ⊢ {M0/x0} r α0 R[x0] = M'' : α'' and Γ ⊢Σ M'' ⇐ A'' and Γ ⊢ (A'')⁻ = α'' ))
            by theorem subst-preserves-atom-term on α0, (R[x0] assumes Γ, x0:A0), at1, arg, sim 
            eqP: Γ ⊢ P' == A'' by theorem subst-unique on sbA, sbA'
            use inversion on eqP where A'' := P'
            proof by case analysis on rest:
                case or sbR: Γ ⊢ {M0/x} r α0 R[x] = R'' and at2: Γ ⊢Σ R'' ⇒ P' is
                    sbM: Γ ⊢ {M0/x0} m α0 R[x0] = R'' by rule Subst-M-R on sbR
                    ct2: Γ ⊢Σ R'' ⇐ P' by rule Canon-Term-Atom on at2
                    proof by sbM, ct2
                end case

                case or sbRH: Γ ⊢ {M0/x} r α0 R[x] = M'' : α'' and 
                    ct2: Γ ⊢ Σ M'' ⇐ P' and
                    sim'': Γ ⊢ (A'')⁻ = α'' 
                is
                    sbM: Γ ⊢ {M0/x} m α0 R[x] = M'' by rule Subst-M-RH on sbRH
                    proof by sbM, ct2
                 end case
            end case analysis
        end case

        case rule
            ct11: Γ, x0 : A0, x' : A1[x0] ⊢ Σ M1[x0][x'] ⇐ A2[x0][x']
            -------------------------------------------------------- Canon-Term-Lam
            _:    Γ, x0 : A0 ⊢ Σ (λ x' • M1[x0][x']) ⇐ (Π x' : A1[x0] • A2[x0][x'])
            where M[x0] := λ x' • M1[x0][x']
             and  A[x0] := Π x' : A1[x0] • A2[x0][x']
        is
            proof by case analysis on sbA:
                case rule
                    sbA1: Γ ⊢ {M0/x} α0 A1[x] = A1'
                    sbA2: Γ, x' : A1' ⊢ {M0/x0} α0 A2[x0][x'] = A2'[x']
                    --------------------------------------------------------------- Subst-A-Pi
                    _: Γ ⊢ {M0/x0} α0 (Π x' : A1[x0] • A2[x0][x']) = (Π x' : A1' • A2'[x'])
                    where A' := Π x' : A1' • A2'[x']
                is
                    // serious problem: ct12 is not true in general:
                    // we are prematurely substituting the type of x' so that we
                    // can exchange it before x0.
                    ct12: Γ, x' : A1', x0 : A0 ⊢Σ M1[x0][x'] ⇐ A2[x0][x'] by unproved //! expected
                    arga: Γ, x' : A1' ⊢Σ M0 ⇐ A0 by weakening on arg
                    sima: Γ, x' : A1' ⊢ (A0)⁻ = α0 by weakening on sim
                    sbM1a: Γ, x':A1' ⊢ {M0/x0} m α0 M1[x0][x'] = M1'[x'] and
                    ct11': Γ, x' : A1' ⊢Σ M1'[x'] ⇐ A2'[x'] by theorem subst-preserves-canon-term on α0, α0, sx, (M1[x0][x'] assumes Γ, x':A1', x0:A0), ct12, arga, sima, sbA2
                    sbM1: Γ, x':0 ⊢ {M0/x0} m α0 M1[x0][x'] = M1'[x'] by lemma subst-ignores-variable-type on sbM1a, (0)
                    sbM: Γ ⊢ {M0/x0} m α0 λ x' • M1[x0][x'] = λ x' • M1'[x'] by rule Subst-M-Lam on sbM1
                    ct2: Γ ⊢Σ λ x' • M1'[x'] ⇐ Π x' : A1' • A2'[x'] by rule Canon-Term-Lam on ct11'
                    proof by sbM, ct2
                end case

            end case analysis
        end case

    end case analysis
end theorem
and
theorem subst-preserves-atom-term:
    assumes Γ
    forall α0
    forall R[x0] assumes Γ, x0:A0
    forall at1: Γ, x0:A0 ⊢Σ R[x0] ⇒ A[x0]
    forall arg: Γ ⊢Σ M0 ⇐ A0
    forall sim: Γ ⊢ (A0)⁻ = α0 
    exists // test complex and/or derivations
     Γ ⊢ {M0/x0} α0 A[x0] = A' and
     ((Γ ⊢ {M0/x0} r α0 R[x0] = R' and
       Γ ⊢Σ R' ⇒ A') or
      (Γ ⊢ {M0/x0} r α0 R[x0] = M' : α' and
       Γ ⊢Σ M' ⇐ A' and
       Γ ⊢ (A')⁻ = α' ))
    use induction on α0, (R[x0] assumes Γ, x0:A0)
    sx: α0 ≤ α0 by rule sub-eq
    proof by case analysis on at1:
        case rule
            in: R[x0] : A[x0] ∈ (Γ, x0 : A0)
            ---------------- Atom-Term-Var
            _: Γ, x0 : A0 ⊢ Σ R[x0] ⇒ A[x0]
        is
            proof by case analysis on in:
                case rule
                    --------------- var
                    _: x0 : A0 ∈ (Γ, x0 : A0)
                    where R[x0] := x0 and A[x0] := A0
                is
                    sbp: Γ ⊢ {M0/x0} α0 A0 = A0 by theorem nop-subst-a on M0, α0, A0
                    sbr: Γ ⊢ {M0/x0} r α0 R[x0] = M0 : α0 by rule Subst-RH-Var
                    proof by sbp, (sbr, arg, sim)
                end case

                case rule
                    --------------- var
                    _: x : A' ∈ (Γ', x : A', x0 : A1[x])
                    where A[x0] := A'
                is
                    sbp: Γ ⊢ {M0/x0} α0 A' = A' by theorem nop-subst-a on M0, α0, A'
                    v: x : A' ∈ (Γ', x : A') by rule var
                    v': R[0] : A' ∈ Γ by v
                    sbr: Γ ⊢ {M0/x0} r α0 R[x0] = R[0] by rule Subst-R-Var on v'
                    at2: Γ ⊢Σ R[0] ⇒ A' by rule Atom-Term-Var on v'
                    proof by sbp, (sbr, at2)
                end case

            end case analysis
        end case

        case rule
            l: c : A' ∈ Σ
            ----------------- Atom-Term-Const
            _: Γ, x0 : A0 ⊢Σ c ⇒ A'
            where R[x0] := c and A[x0] := A'
        is
            sbp: Γ ⊢ {M0/x0} α0 A' = A' by theorem nop-subst-a on M0, α0, A'
            sbr: Γ ⊢ {M0/x0} r α0 c = c by rule Subst-R-Const
            at2: Γ ⊢Σ c ⇒ A' by rule Atom-Term-Const on l
            proof by sbp, (sbr, at2)
        end case

        case rule
            atf: Γ, x0 : A0 ⊢Σ R1[x0] ⇒ (Π x' : A1[x0] • A2[x0][x'])
            cta: Γ, x0 : A0 ⊢Σ M1[x0] ⇐ A1[x0]
            sta: Γ, x0 : A0 ⊢ (A1[x0])⁻ = α1
            sbt: Γ, x0 : A0 ⊢ {M1[x0]/x'} α1 A2[x0][x'] = A[x0]
            ----------------------------------------------- Atom-Term-App
            _: Γ, x0 : A0 ⊢Σ (R1[x0] M1[x0]) ⇒ A[x0]
            where R[x0] := R1[x0] M1[x0]
        is
            sbft: Γ ⊢ {M0/x0} α0 (Π x' : A1[x0] • A2[x0][x']) = A12' and
            orf: ((Γ ⊢ {M0/x0} r α0 R1[x0] = R1' and
                  Γ ⊢Σ R1' ⇒ A12') or
                 (Γ ⊢ {M0/x0} r α0 R1[x0] = M12' : α12' and
                  Γ ⊢Σ M12' ⇐ A12' and
                  Γ ⊢ (A12')⁻ = α12' )) by induction hypothesis on α0, (R1[x0] assumes Γ, x0:A0), atf, arg, sim
            sbA1: Γ ⊢ {M0/x0} α0 A1[x0] = A1' and
            sbA2: Γ, x' : A1' ⊢ {M0/x0} α0 A2[x0][x'] = A2'[x'] by inversion on sbft
            where A12' := Π x' : A1' • A2'[x']
            sta': Γ ⊢ (A1')⁻ = α1 by theorem subst-a-preserves-erasure on sta, sbA1
            sbM1: Γ ⊢ {M0/x0} m α0 M1[x0] = M1' and
            ctM1: Γ ⊢Σ M1' ⇐ A1' by theorem subst-preserves-canon-term on α0, α0, sx, (M1[x0] assumes Γ, x0:A0), cta, arg, sim, sbA1
            sbA: Γ ⊢ {M0/x0} α0 A[x0] = A' and
            sbA2': Γ ⊢ {M1'/x} α1 A2'[x] = A' by theorem subst-a-compose on sbM1, sbt, sbA2
            proof by case analysis on orf:
                case or sbf: Γ ⊢ {M0/x0} r α0 R1[x0] = R1' and 
                    atf': Γ ⊢Σ R1' ⇒ (Π x : A1' • A2'[x]) 
                is
                    sb2: Γ ⊢ {M0/x0} r α0 R1[x0] M1[x0] = R1' M1' by rule Subst-R-App on sbf, sbM1
                    at2: Γ ⊢Σ R1' M1' ⇒ A' by rule Atom-Term-App on atf', ctM1, sta', sbA2'
                    proof by sbA, (sb2, at2)
                end case

                case or sbf: Γ ⊢ {M0/x0} r α0 R1[x0] = M12' : α12' and 
                    ctf': Γ ⊢Σ M12' ⇐ (Π x1 : A1' • A2'[x1]) and 
                    sm12: Γ ⊢ (Π x1 : A1' • A2'[x1])⁻ = α12' 
                is
                    sx12: α12' ≤ α0 by theorem head-substitution-size on sbf
                    ctb: Γ, x1 : A1' ⊢Σ M2[x1] ⇐ A2'[x1] by inversion on ctf'
                    where M12' := λ x1 • M2[x1]
                    eq12: α12' = α1 → α' and 
                    sim2: Γ, x1:A1' ⊢ (A2'[x1])⁻ = α' by lemma simple-prod-inversion on sm12, sta'
                    use inversion on eq12 where α12' := α1 → α'
                    sim': Γ ⊢ (A')⁻ = α' by theorem subst-a-preserves-erasure on sim2, sbA2'
                    sx1: α1 ≤ α1 by rule sub-eq
                    sx1': α1 ≤ α1 → α' by rule sub-arrow1 on sx1
                    sbM2 : Γ ⊢ {M1'/x1} m α1 M2[x1] = M' and
                    ct2': Γ ⊢Σ M' ⇐ A' by case analysis on sx12:
                        case rule
                            ----------- sub-eq
                            _: (α1 → α') ≤ (α1 → α')
                            where α0 := α1 → α'
                        is
                            proof by theorem subst-preserves-canon-term on α1, α1, sx1, (M2[x1] assumes Γ, x1:A1'), ctb, ctM1, sta', sbA2'
                        end case

                        case rule
                            sx2: (α1 → α') ≤ α2
                            ------------------- sub-arrow1
                            _: (α1 → α') ≤ (α2 → α3)
                            where α0 := α2 → α3
                        is
                            sx2': α1 ≤ α2 by lemma subexpression-transitive on sx1', sx2
                            proof by theorem subst-preserves-canon-term on α1, α2, sx2', (M2[x1] assumes Γ, x1:A1'), ctb, ctM1, sta', sbA2'
                        end case

                        case rule
                            sx3: (α1 → α') ≤ α3
                            ------------------- sub-arrow2
                            _: (α1 → α') ≤ (α2 → α3)
                            where α0 := α2 → α3
                        is
                            sx3': α1 ≤ α3 by lemma subexpression-transitive on sx1', sx3
                            proof by theorem subst-preserves-canon-term on α1, α3, sx3', (M2[x1] assumes Γ, x1:A1'), ctb, ctM1, sta', sbA2'
                        end case
                    end case analysis
                    sb2: Γ ⊢ {M0/x0} r α0 R1[x0] M1[x0] = M' : α' by rule Subst-RH-App on sbf, sbM1, sbM2
                    proof by sbA, (sb2, ct2', sim')
                end case
            end case analysis
        end case

    end case analysis
end theorem
