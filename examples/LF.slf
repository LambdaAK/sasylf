// Lemma 2.3 (4)
theorem atom-term-unique:
    assumes Γ
    forall ok: ⊢ Σ sig
    forall d1: Γ ⊢Σ R ⇒ A'
    forall d2: Γ ⊢Σ R ⇒ A''
    exists Γ ⊢ A' == A''
    do case analysis on d2:
        case rule
            v2: R : A'' ∈ Γ
            ---------------- Atom-Term-Var
            _: Γ ⊢ Σ R ⇒ A''
        is
            proof by case analysis on v2:
                case rule
                    ------------------------- var
                    _: x : A'' ∈ (Γ', x : A'')
                is
                    v1: R : A' ∈ Γ by inversion on d1
                    use inversion on v1 where A' := A''
                    proof by rule term-eq
                end case
            end case analysis
        end case
    end case analysis
    proof by induction on d1:
        case rule
            v1: R : A' ∈ Γ
            ---------------- Atom-Term-Var
            _: Γ ⊢ Σ R ⇒ A'
        is
            proof by case analysis on v1:
                case rule
                    --------------- var
                    _: x : A' ∈ (Γ', x : A')
                is
                    proof by contradiction on d2
                end case
            end case analysis
        end case

        case rule
            l1: c : A' ∈ Σ
            ----------------- Atom-Term-Const
            _: Γ ⊢ Σ c ⇒ A'
            where R := c
        is
            l2: c : A'' ∈ Σ by inversion on d2
            eq: • ⊢ A' == A'' by lemma good-sig-con-unique on ok, l1, l2
            proof by weakening on eq
        end case

        case rule
            r1: Γ ⊢Σ R1 ⇒ (Π x : A1' • A2'[x])
            _:  Γ ⊢Σ M1 ⇐ A1'
            t1:  Γ ⊢ (A1')⁻ = α'
            s1: Γ ⊢ ({M1/x} α' A2'[x]) = A'
            --------------------------- Atom-Term-App
            _: Γ ⊢Σ (R1 M1) ⇒ A'
            where R := R1 M1
        is
            r2: Γ ⊢ Σ R1 ⇒ (Π x : A1'' • A2''[x]) and
            _: Γ ⊢ Σ M1 ⇐ A1'' and
            t2: Γ ⊢ (A1'')⁻ = α'' and
            s2: Γ ⊢ ({M1/x} α'' A2''[x]) = A'' by inversion on d2
            eqF: Γ ⊢ (Π x : A1' • A2'[x]) == (Π x : A1'' • A2''[x]) by induction hypothesis on ok, r1, r2
            use inversion on eqF where A1' := A1'' and A2'[x] := A2''[x]
            eqST: α' = α'' by lemma simple-type-unique on t1, t2
            use inversion on eqST where α' := α''
            proof by theorem subst-unique on s1, s2
        end case

    end induction
end theorem



//// Decidability of substitution

// We want to prove that substitution terminates: either it exists or it is
// not possible.  In the truncated Twelf proof (which doesn't model signatures or
// subordination), it just proves "termination", but that doesn't give any
// ability to use substitution in anything.

// Here, in contrast, we give a definition of "impossible" and prove that
// either substitution is possible or that it's not possible.
// First, we show that our definition of impossible doesn't overlap
// with possible substitution

judgment simple-not-equal: α ≠ α

     n != n'
     ------- ne-base
     n ≠ n'
     
     ----------- ne-base-arrow
     a ≠ (α → α')
         
     ------------ ne-arrow-base
     (α → α') ≠ a
     
           α1 ≠ α2          
     ---------------------- ne-arrow-1
     (α1 → α1') ≠ (α2 → α2')
     
           α1' ≠ α2'          
     ---------------------- ne-arrow-2
     (α1 → α1') ≠ (α2 → α2')
     
theorem simple-not-equal-anti-reflexive:
    forall ne: α ≠ α
    exists contradiction
    proof by induction on ne:
        case rule
            ne1: n != n
            ------------- ne-base
            _: n ≠ n
            where α := n
        is
            proof by theorem Natural.ne-anti-reflexive on ne1
        end case

        case rule
            ne1: α1 ≠ α1
            ---------------------------- ne-arrow-1
            _: (α1 → α1') ≠ (α1 → α1')
            where α := (α1 → α1')
        is
            proof by induction hypothesis on ne1
        end case

        case rule
            ne1: α1' ≠ α1'
            ---------------------------- ne-arrow-2
            _: (α1 → α1') ≠ (α1 → α1')
            where α := (α1 → α1')
        is
            proof by induction hypothesis on ne1
        end case
    end induction
end theorem

lemma simple-eq-decidable:
    forall α1
    forall α2
    exists α1 = α2 or α1 ≠ α2
    proof by induction on α1:
        case a1 is
            proof by case analysis on α2:
                case a2 is
                    use inversion on a1 where a1 := n1
                    use inversion on a2 where a2 := n2
                    ch: n1 == n2 or n1 != n2 by theorem Natural.eq-or-ne on n1, n2
                    proof by case analysis on ch:
                        case or eq: n1 == n2 is
                            use inversion on eq where n1 := n2
                            _: α1 = α2 by rule simple-eq
                        end case

                        case or ne: n1 != n2 is
                            _: a1 ≠ a2 by rule ne-base on ne
                        end case
                    end case analysis
                end case

                case α21 → α22 is
                    _: a1 ≠ α21 → α22 by rule ne-base-arrow
                end case

            end case analysis
        end case

        case α11 → α12 is
            proof by case analysis on α2:
                case a2 is
                    _: α11 → α12 ≠ a2 by rule ne-arrow-base
                end case

                case α21 → α22 is
                    ch1: α11 = α21 or α11 ≠ α21 by induction hypothesis on α11, α21
                    do case analysis on ch1:
                        case or ne1: α11 ≠ α21 is
                            _: α11 → α12 ≠ α21 → α22 by rule ne-arrow-1 on ne1
                        end case
                    end case analysis
                    eq1: α11 = α21 by inversion on ch1
                    use inversion on eq1 where α21 := α11
                    ch2: α12 = α22 or α12 ≠ α22 by induction hypothesis on α12, α22
                    do case analysis on ch2:
                        case or ne2: α12 ≠ α22 is
                            _: α11 → α12 ≠ α21 → α22 by rule ne-arrow-2 on ne2
                        end case
                    end case analysis
                    eq2: α12 = α22 by inversion on ch2
                    use inversion on eq2 where α22 := α12
                    _: α11 → α12 = α11 → α12 by rule simple-eq
                end case
            end case analysis
        end case
    end induction
end lemma


terminals undefined

judgment no-subst: Γ ⊢ σ undefined
assumes Γ

  Γ ⊢ {M0/x0} α A[x0] undefined
  ---------------------------------------------------- No-Subst-K-Pi-1
  Γ ⊢ {M0/x0} α (Π x:A[x0] • K[x0][x]) undefined

  Γ ⊢ {M0/x0} α A[x0] = A'
  Γ, x:A' ⊢ {M0/x0} α K[x0][x] undefined
  ---------------------------------------------------- No-Subst-K-Pi-2
  Γ ⊢ {M0/x0} α (Π x:A[x0] • K[x0][x]) undefined

  Γ ⊢ {M0/x0} p α P[x0] undefined
  ----------------------------- No-Subst-A-P
  Γ ⊢ {M0/x0}   α P[x0] undefined
  
  Γ ⊢ {M0/x0} α A1[x0] undefined
  ---------------------------------------------- No-Subst-A-Pi-1
  Γ ⊢ {M0/x0} α (Π x:A1[x0] • A2[x0][x]) undefined
  
  Γ ⊢ {M0/x0} α A1[x0] = A1'
  Γ, x:A1' ⊢ {M0/x0} α A2[x0][x] undefined
  ---------------------------------------------- No-Subst-A-Pi-2
  Γ ⊢ {M0/x0} α (Π x:A1[x0] • A2[x0][x]) undefined
  
  Γ ⊢ {M0/x0} p α P[x0] undefined
  ----------------------------------- No-Subst-P-App-1
  Γ ⊢ {M0/x0} p α P[x0] M[x0] undefined
  
  Γ ⊢ {M0/x0} m α M[x0] undefined
  ----------------------------------- No-Subst-P-App-2
  Γ ⊢ {M0/x0} p α P[x0] M[x0] undefined

  Γ ⊢ {M0/x0} r α R[x0] undefined
  ------------------------------ No-Subst-M-R
  Γ ⊢ {M0/x0} m α R[x0] undefined

  Γ, x:0 ⊢ {M0/x0} m α M[x0][x] undefined
  -------------------------------------------- No-Subst-M-Lam 
  Γ ⊢ {M0/x0} m α (λ x • M[x0][x]) undefined

  Γ ⊢ {M0/x0} r α0 R1[x0] undefined
  -------------------------------------- No-Subst-R-App-1
  Γ ⊢ {M0/x0} r α0 R1[x0] M2[x0] undefined

  Γ ⊢ {M0/x0} m α0 M2[x0] undefined
  -------------------------------------- No-Subst-R-App-2
  Γ ⊢ {M0/x0} r α0 R1[x0] M2[x0] undefined

  Γ ⊢ {M0/x0} r α0 R1[x0] = M1 : a
  ------------------------------------- No-Subst-RH-App-Base
  Γ ⊢ {M0/x0} r α0 R1[x0] M2[x0] undefined
  
  Γ ⊢ {M0/x0} r α0 R1[x0] = R' : α
  ------------------------------------- No-Subst-RH-App-Prim
  Γ ⊢ {M0/x0} r α0 R1[x0] M2[x0] undefined
  
  Γ ⊢ {M0/x0} r α0 R1[x0] = λ x • M1'[x] : α2 → α
  Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
  Γ ⊢ {M2'/x} m α2 M1'[x] undefined
  ------------------------------------- No-Subst-RH-App-3
  Γ ⊢ {M0/x0} r α0 R1[x0] M2[x0] undefined
  
theorem no-subst-implies-subst-impossible:
    assumes Γ
    forall ns: Γ ⊢ σ undefined
    forall sb: Γ ⊢ σ = E'
    exists contradiction
    use induction on ns
    proof by case analysis on sb:
                
        case rule
            ------------------------------ Subst-K-Type
            _: Γ ⊢ ({M0/x0} α0 type) = type
            where σ := {M0/x0} α0 type and E' := type
        is
            proof by contradiction on ns
        end case

        case rule
            sba: Γ ⊢ ({M0/x0} α0 A1[x0]) = A1'
            sbb: Γ, x : A1' ⊢ ({M0/x0} α0 K1[x0][x]) = K1'[x]
            ---------------------------------------------------------- Subst-K-Pi
            _: Γ ⊢ ({M0/x0} α0 (Π x : A1[x0] • K1[x0][x])) = (Π x : A1' • K1'[x])
            where σ := {M0/x0} α0 (Π x : A1[x0] • K1[x0][x]) and E' := Π x : A1' • K1'[x]
        is
            proof by case analysis on ns:
                case rule
                    nsa: Γ ⊢ ({M0/x} α0 A1[x]) undefined
                    ---------------------------------------------------------- No-Subst-K-Pi-1
                    _: Γ ⊢ ({M0/x} α0 (Π x' : A1[x] • K1[x][x'])) undefined
                is
                    proof by induction hypothesis on nsa, sba
                end case

                case rule
                    sba2: Γ ⊢ ({M0/x} α0 A1[x]) = A1''
                    nsb: Γ, x : A1'' ⊢ ({M0/x'} α0 K1[x'][x]) undefined
                    ---------------------------------------------------------- No-Subst-K-Pi-2
                    _: Γ ⊢ ({M0/x} α0 (Π x' : A1[x] • K1[x][x'])) undefined
                is
                    eqA: Γ ⊢ A1' == A1'' by theorem subst-unique on sba, sba2
                    use inversion on eqA where A1' := A1''
                    proof by induction hypothesis on nsb, sbb
                end case

            end case analysis
       end case
            
       case rule
            sba: Γ ⊢ ({M0/x0} α0 A1[x0]) = A1'
            sbb: Γ, x : A1' ⊢ ({M0/x0} α0 A2[x0][x]) = A2'[x]
            --------------------------------------------------------------- Subst-A-Pi
            _: Γ ⊢ ({M0/x0} α0 (Π x : A1[x0] • A2[x0][x])) = (Π x : A1' • A2'[x])
            where σ := {M0/x0} α0 (Π x : A1[x0] • A2[x0][x]) and E' := Π x : A1' • A2'[x]
        is
            proof by case analysis on ns:
                case rule
                    nsa: Γ ⊢ ({M0/x} α0 A1[x]) undefined
                    ---------------------------------------------------- No-Subst-A-Pi-1
                    _: Γ ⊢ ({M0/x} α0 (Π x' : A1[x] • A2[x][x'])) undefined
                is
                    proof by induction hypothesis on nsa, sba
                end case

                case rule
                    sba': Γ ⊢ ({M0/x0} α0 A1[x0]) = A1''
                    nsb: Γ, x : A1'' ⊢ ({M0/x0} α0 A2[x0][x]) undefined
                    ---------------------------------------------------- No-Subst-A-Pi-2
                    _: Γ ⊢ ({M0/x0} α0 (Π x : A1[x0] • A2[x0][x])) undefined
                is
                    eqA: Γ ⊢ A1' == A1'' by theorem subst-unique on sba, sba'
                    use inversion on eqA where A1' := A1''
                    proof by induction hypothesis on nsb, sbb
                end case

            end case analysis
        end case

        case rule
            sbp: Γ ⊢ ({M0/x} p α0 P[x]) = P'
            ------------------------------ Subst-A-P
            _: Γ ⊢ ({M0/x} α0 P[x]) = P'
            where σ := {M0/x0} α0 P[x0] and E' := P'
        is
            nsp: Γ ⊢ {M0/x0} p α0 P[x0] undefined by inversion on ns
            proof by induction hypothesis on nsp, sbp
        end case

        case rule
            ----------------------- Subst-P-Fam
            _: Γ ⊢ {M0/x0} p α0 a = a
            where σ := {M0/x0} p α0 a and E' := a
        is
            proof by contradiction on ns
        end case
        
        case rule
            sbf: Γ ⊢ {M0/x0} p α0 P1[x0] = P1'
            sba: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
            -------------------------------------------- Subst-P-App
            _: Γ ⊢ {M0/x0} p α0 (P1[x0] M2[x0]) = P1' M2'
            where σ := {M0/x0} p α0 (P1[x0] M2[x0]) and E' := P1' M2'
        is
            proof by case analysis on ns:
                case rule
                    nsf: Γ ⊢ ({M0/x} p α0 P1[x]) undefined
                    ----------------------------------------- No-Subst-P-App-1
                    _: Γ ⊢ ({M0/x} p α0 (P1[x] M2[x])) undefined
                is
                    proof by induction hypothesis on nsf, sbf
                end case

                case rule
                    nsa: Γ ⊢ ({M0/x} m α0 M2[x]) undefined
                    ----------------------------------------- No-Subst-P-App-2
                    _: Γ ⊢ ({M0/x} p α0 (P1[x] M2[x])) undefined
                is
                    proof by induction hypothesis on nsa, sba
                end case
            end case analysis
        end case
            
        case rule
            sb1: Γ ⊢ ({M0/x0} r α0 R[x0]) = R'
            ------------------------------- Subst-M-R
            _:   Γ ⊢ ({M0/x0} m α0 R[x0]) = R'
            where σ := {M0/x0} m α0 R[x0] and E' := R'
        is
            ns1: Γ ⊢ ({M0/x} r α0 R[x]) undefined by inversion on ns
            proof by induction hypothesis on ns1, sb1
        end case

        case rule
            srh: Γ ⊢ ({M0/x0} r α0 R[x0]) = M' : α1
            ------------------------------------ Subst-M-RH
            _:   Γ ⊢ ({M0/x0} m α0 R[x0]) = M'
            where σ := {M0/x0} m α0 R[x0] and E' := M'
        is
            ns1: Γ ⊢ ({M0/x} r α0 R[x]) undefined by inversion on ns
            proof by theorem no-subst-implies-subst-h-impossible on ns1, srh
        end case

        case rule
            sb1: Γ, x : 0 ⊢ ({M0/x0} m α0 M1[x0][x]) = M1'[x]
            -------------------------------------------------- Subst-M-Lam
            _: Γ ⊢ ({M0/x0} m α0 (λ x • M1[x0][x])) = λ x • M1'[x]
            where σ := {M0/x0} m α0 (λ x • M1[x0][x]) and E' := λ x • M1'[x]
        is
            ns1: Γ, x : 0 ⊢ ({M0/x0} m α0 M1[x0][x]) undefined by inversion on ns
            proof by induction hypothesis on ns1, sb1
        end case
        
        case rule
            v: R:A ∈ Γ
            ---------------- Subst-R-Var
            _: Γ ⊢ {M0/x0} r α0 R = R
            where σ := {M0/x0} r α0 R and E' := R
        is
            proof by case analysis on v:
                case rule
                    ------------------- var
                    _: x : A ∈ (Γ', x : A)
                is
                    proof by contradiction on ns
                end case
            end case analysis
        end case
        
        case rule
            --------------------------- Subst-R-Const
            _: Γ ⊢ ({M0/x0} r α0 c) = c
            where σ := {M0/x0} r α0 c and E' := c
        is
            proof by contradiction on ns
        end case
        
        case rule
            sbf: Γ ⊢ ({M0/x0} r α0 R0[x0]) = R1
            sba: Γ ⊢ ({M0/x0} m α0 M1[x0]) = M2
            ------------------------------------------------------- Subst-R-App
            _: Γ ⊢ {M0/x0} r α0 (R0[x0] M1[x0]) = R1 M2
            where σ := {M0/x0} r α0 (R0[x0] M1[x0]) and E' := R1 M2
        is
            proof by case analysis on ns:
                case rule
                    unf: Γ ⊢ ({M0/x} r α0 R0[x]) undefined
                    -------------------------------------------- No-Subst-R-App-1
                    _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
                is
                    proof by induction hypothesis on unf, sbf
                end case

                case rule
                    una: Γ ⊢ ({M0/x} m α0 M1[x]) undefined
                    -------------------------------------------- No-Subst-R-App-2
                    _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
                is
                    proof by induction hypothesis on una, sba
                end case

                case rule
                    srh: Γ ⊢ ({M0/x} r α0 R0[x]) = M0' : a0
                    ------------------------------------------- No-Subst-RH-App-Base
                    _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
                is
                    proof by theorem subst-r-rh-incompatible on sbf, srh
                end case

                case rule
                    srh: Γ ⊢ ({M0/x} r α0 R0[x]) = R2 : α1
                    ------------------------------------------- No-Subst-RH-App-Prim
                    _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
                is
                    proof by theorem subst-r-rh-incompatible on sbf, srh
                end case

                case rule
                    srh: Γ ⊢ ({M0/x} r α0 R0[x]) = (λ x • M3[x]) : (α1 → α2)
                    _: Γ ⊢ ({M0/x} m α0 M1[x]) = M4
                    _: Γ ⊢ ({M4/x} m α1 M3[x]) undefined
                    ------------------------------------------- No-Subst-RH-App-3
                    _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
                is
                    proof by theorem subst-r-rh-incompatible on sbf, srh
                end case
            end case analysis
        end case
    end case analysis
end theorem
and
theorem no-subst-implies-subst-h-impossible:
    assumes Γ
    forall ns: Γ ⊢ σ undefined
    forall sb: Γ ⊢ σ = M' : α'
    exists contradiction
    use induction on ns
    do case analysis on sb:
        case rule
            ------------------------------------ Subst-RH-Var
            _: Γ ⊢ ({M0/x} r α0 x) = M0 : α0
            where σ := {M0/x0} r α0 x0 and M' := M0 and α' := α0
        is
            proof by contradiction on ns
        end case
    end case analysis
    srh:Γ ⊢ ({M0/x0} r α0 R0[x0]) = λ x • M2[x] : α2 → α' and
    sba:Γ ⊢ ({M0/x0} m α0 M1[x0]) = M20 and
    sb3:Γ ⊢ ({M20/x} m α2 M2[x]) = M' by inversion on sb
    where σ := {M0/x0} r α0 (R0[x0] M1[x0])
    proof by induction on ns:
        case rule
            ns1: Γ ⊢ ({M0/x0} r α0 R0[x0]) undefined
            -------------------------------------------- No-Subst-R-App-1
            _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
        is
            proof by induction hypothesis on ns1, srh
        end case

        case rule
            ns1: Γ ⊢ ({M0/x} m α0 M1[x]) undefined
            -------------------------------------------- No-Subst-R-App-2
            _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
        is
            proof by theorem no-subst-implies-subst-impossible on ns1, sba
        end case

        case rule
            sb1: Γ ⊢ ({M0/x} r α0 R0[x]) = M0' : a
            ------------------------------------------- No-Subst-RH-App-Base
            _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
        is
            eqL: Γ ⊢ M0' == (λ x • M2[x]) and
            eqS: a = α2 → α' by theorem subst-h-unique on sb1, srh
            proof by contradiction on eqS
        end case

        case rule
            sb1: Γ ⊢ ({M0/x} r α0 R0[x]) = R1 : α1
            ------------------------------------------- No-Subst-RH-App-Prim
            _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
        is
            eqL: Γ ⊢ R1 == (λ x • M2[x]) and
            eqS: α1 = α2 → α' by theorem subst-h-unique on sb1, srh
            proof by contradiction on eqL
        end case

        case rule
            sb11: Γ ⊢ ({M0/x} r α0 R0[x]) = (λ x • M3'[x]) : (α2' → α'')
            sb12: Γ ⊢ ({M0/x} m α0 M1[x]) = M3
            sb13: Γ ⊢ ({M3/x} m α2' M3'[x]) undefined
            ------------------------------------------- No-Subst-RH-App-3
            _: Γ ⊢ ({M0/x} r α0 (R0[x] M1[x])) undefined
        is
             eqL: Γ ⊢ λ x • M3'[x] == (λ x • M2[x]) and
            eqS: α2' → α'' = α2 → α' by theorem subst-h-unique on sb11, srh
            use inversion on eqL where M3'[x] := M2[x]
            use inversion on eqS where α2' := α2 and α'' := α'
            eqM: Γ ⊢ M3 == M20 by theorem subst-unique on sb12, sba
            use inversion on eqM where M3 := M20
            proof by theorem no-subst-implies-subst-impossible on sb13, sb3
        end case
    end induction    
end theorem



/// Theorem 2.4 (Decidability of Substitution)

// We prove first prove with mutual induction the m and r parts
// Then afterwards prove the p, a and k parts

theorem subst-m-decidable:
    assumes Γ
    forall M[x0] assumes Γ, x0 : A0
    forall M0 assumes Γ
    forall α
    forall subterm: α0 ≤ α
    exists Γ ⊢ {M0/x0} m α0 M[x0] = M' or Γ ⊢ {M0/x0} m α0 M[x0] undefined
    use induction on α, (M[x0] assumes Γ, x0 : A0)
    // handle induction if α0 < α
    do case analysis on subterm:
        case rule
            sub': α0 ≤ α1
            ----------------- sub-arrow1
            _: α0 ≤ α1 → α2
            where α := α1 → α2
        is
            proof by induction hypothesis on (M[x0] assumes Γ, x0 : A0), M0, α1, sub'
        end case
        case rule
            sub': α0 ≤ α2
            ----------------- sub-arrow2
            _: α0 ≤ α1 → α2
            where α := α1 → α2
        is
            proof by induction hypothesis on (M[x0] assumes Γ, x0 : A0), M0, α2, sub'
        end case
    end case analysis
    use inversion on subterm where α0 := α
    proof by case analysis on (M[x0] assumes Γ, x0 : A0):
        case R[x] assumes Γ, x : A0 is
            ch: Γ ⊢ {M0/x0} r α0 R[x0] = R' or Γ ⊢ {M0/x0} r α0 R[x0] = M0' : α' or Γ ⊢ {M0/x0} r α0 R[x0] undefined by theorem subst-r-decidable on (R[x0] assumes Γ, x0 : A0), M0, α0
            proof by case analysis on ch:
                case or sb: Γ ⊢ ({M0/x0} r α R[x0]) = R' is
                    _: Γ ⊢ {M0/x0} m α0 R[x0] = R' by rule Subst-M-R on sb
                end case

                case or sb: Γ ⊢ ({M0/x} r α R[x]) = M0' : α' is
                    _: Γ ⊢ {M0/x0} m α0 R[x0] = M0' by rule Subst-M-RH on sb
                end case

                case or u: Γ ⊢ ({M0/x} r α R[x]) undefined is
                    _: Γ ⊢ {M0/x0} m α0 R[x0] undefined by rule No-Subst-M-R on u
                end case

            end case analysis
        end case

        case λ x • M1[x0][x] assumes Γ, x0 : A0 is
            ch: Γ, x:0 ⊢ {M0/x0} m α0 M1[x0][x] = M1'[x] or Γ, x:0 ⊢ {M0/x0} m α0 M1[x0][x] undefined by induction hypothesis on (M1[x0][x] assumes Γ, x:0, x0:A0), M0, α0, subterm            
            proof by case analysis on ch:
                case or subb: Γ, x : 0 ⊢ ({M0/x0} m α M1[x0][x]) = M1'[x] is
                    _: Γ ⊢ {M0/x0} m α0 (λ x • M1[x0][x]) = λ x • M1'[x] by rule Subst-M-Lam on subb
                end case

                case or ub: Γ, x : 0 ⊢ ({M0/x0} m α M1[x0][x]) undefined is
                    _: Γ ⊢ {M0/x0} m α0 (λ x • M1[x0][x]) undefined by rule No-Subst-M-Lam on ub
                end case
            end case analysis
        end case

    end case analysis
end theorem
and
theorem subst-r-decidable:
    assumes Γ
    forall R[x0] assumes Γ, x0 : A0
    forall M0 assumes Γ
    forall α0
    exists Γ ⊢ {M0/x0} r α0 R[x0] = R' or Γ ⊢ {M0/x0} r α0 R[x0] = M':α' or Γ ⊢ {M0/x0} r α0 R[x0] undefined
    use induction on α0, (R[x0] assumes Γ, x0 : A0)
    proof by case analysis on (R[x0] assumes Γ, x0 : A0):
        case x0 assumes Γ, x0 : A0 is
            _: Γ ⊢ {M0/x0} r α0 (x0) = M0:α0 by rule Subst-RH-Var
        end case

        case x assumes Γ', x : A, x0 : A0 is
            v1: x:A ∈ Γ', x:A by rule var
            v2: R[0]:A ∈ Γ by v1 
            _: Γ ⊢ {M0/x0} r α0 R[x0] = R[0] by rule Subst-R-Var on v2
        end case

        case c assumes Γ, x0 : A0 is
            _: Γ ⊢ {M0/x0} r α0 c = c by rule Subst-R-Const
        end case

        case R0[x0] M1[x0] assumes Γ, x0 : A0 is
            ch1: Γ ⊢ {M0/x0} r α0 R0[x0] = R0' or Γ ⊢ {M0/x0} r α0 R0[x0] = M0':α0' or Γ ⊢ {M0/x0} r α0 R0[x0] undefined by induction hypothesis on (R0[x0] assumes Γ, x0:A0), M0, α0
            sb0: α0 ≤ α0 by rule sub-eq
            ch2: Γ ⊢ {M0/x0} m α0 M1[x0] = M1' or Γ ⊢ {M0/x0} m α0 M1[x0] undefined by theorem subst-m-decidable on (M1[x0] assumes Γ, x0:A0), M0, α0, sb0
            do case analysis on ch2:
                case or u1: Γ ⊢ {M0/x0} m α0 M1[x0] undefined is
                    _: Γ ⊢ ({M0/x} r α0 R0[x] M1[x]) undefined by rule No-Subst-R-App-2 on u1
                end case
            end case analysis
            suba: Γ ⊢ {M0/x0} m α0 M1[x0] = M1'  by inversion on ch2
            proof by case analysis on ch1:
                case or subf: Γ ⊢ ({M0/x0} r α0 R0[x0]) = R0' is
                    _: Γ ⊢ {M0/x0} r α0 R0[x0] M1[x0] = R0' M1' by rule Subst-R-App on subf, suba
                end case

                case or subf: Γ ⊢ ({M0/x0} r α0 R0[x0]) = M0' : α0' is
                    do case analysis on M0':
                        case R00' is
                            _: Γ ⊢ ({M0/x0} r α0 R0[x0] M1[x0]) undefined by rule No-Subst-RH-App-Prim on subf
                        end case
                    end case analysis
                    use inversion on M0' where M0' := λ x • M2[x]
                    do case analysis on α0':
                        case a0' is
                            _: Γ ⊢ ({M0/x0} r α0 R0[x0] M1[x0]) undefined by rule No-Subst-RH-App-Base on subf
                        end case
                    end case analysis
                    use inversion on α0' where α0' := α2 → α'
                    subex: α2 → α' ≤ α0 by theorem head-substitution-size on subf
                    sub22: α2 ≤ α2 by rule sub-eq
                    sub22': α2 ≤ α2 → α' by rule sub-arrow1 on sub22
                    ch3: Γ ⊢ {M1'/x} m α2 M2[x] = M' or Γ ⊢ {M1'/x} m α2 M2[x] undefined by case analysis on subex:
                        case rule
                            ------------------------ sub-eq
                            _: (α2 → α') ≤ (α2 → α')
                            where α0 := α2 → α' 
                        is
                            proof by theorem subst-m-decidable on (M2[x] assumes Γ, x:A2), M1', α2, sub22
                        end case

                        case rule
                            sub21: (α2 → α') ≤ α01
                            ------------------- sub-arrow1
                            _: (α2 → α') ≤ (α01 → α02)
                            where α0 := α01 → α02
                        is
                            subex': α2 ≤ α01 by lemma subexpression-transitive on sub22', sub21
                            proof by theorem subst-m-decidable on (M2[x] assumes Γ, x:A2), M1', α01, subex'
                        end case

                        case rule
                            sub202: (α2 → α') ≤ α02
                            ------------------- sub-arrow2
                            _: (α2 → α') ≤ (α01 → α02)
                            where α0 := α01 → α02
                        is
                            subex': α2 ≤ α02 by lemma subexpression-transitive on sub22', sub202
                            proof by theorem subst-m-decidable on (M2[x] assumes Γ, x:A2), M1', α02, subex'
                        end case
                    end case analysis
                    proof by case analysis on ch3:
                        case or sub3: Γ ⊢ ({M1'/x} m α2 M2[x]) = M' is
                            _: Γ ⊢ ({M0/x0} r α0 R0[x0] M1[x0]) = M' : α' by rule Subst-RH-App on subf, suba, sub3
                        end case

                        case or u3: Γ ⊢ ({M1'/x} m α2 M2[x]) undefined is
                            _: Γ ⊢ ({M0/x0} r α0 R0[x0] M1[x0]) undefined  by rule No-Subst-RH-App-3 on subf, suba, u3
                        end case

                    end case analysis
                end case

                case or u0: Γ ⊢ ({M0/x} r α0 R0[x]) undefined is
                    _: Γ ⊢ ({M0/x} r α0 R0[x] M1[x]) undefined by rule No-Subst-R-App-1 on u0
                end case

            end case analysis
        end case

    end case analysis
end theorem
    
theorem subst-p-decidable:
    assumes Γ
    forall P[x0] assumes Γ, x0 : A0
    forall M0 assumes Γ
    forall α0
    exists Γ ⊢ {M0/x0} p α0 P[x0] = P' or Γ ⊢ {M0/x0} p α0 P[x0] undefined
    proof by induction on (P[x0] assumes Γ, x0 : A0):
        case a assumes Γ, x : A0 is
            _: Γ ⊢ {M0/x0} p α0 a = a by rule Subst-P-Fam
        end case

        case P1[x] M[x] assumes Γ, x : A0 is
            ch1: Γ ⊢ {M0/x0} p α0 P1[x0] = P1' or Γ ⊢ {M0/x0} p α0 P1[x0] undefined by induction hypothesis on (P1[x0] assumes Γ, x0:A0), M0, α0
            do case analysis on ch1:
                case or uf: Γ ⊢ {M0/x0} p α0 P1[x0] undefined is
                    _: Γ ⊢ {M0/x0} p α0 P1[x0] M[x0] undefined by rule No-Subst-P-App-1 on uf
                end case
            end case analysis
            subf: Γ ⊢ {M0/x0} p α0 P1[x0] = P1' by inversion on ch1
            subexp: α0 ≤ α0 by rule sub-eq
            ch2: Γ ⊢ {M0/x0} m α0 M[x0] = M' or Γ ⊢ {M0/x0} m α0 M[x0] undefined by theorem subst-m-decidable on (M[x0] assumes Γ, x0:A0), M0, α0, subexp 
            do case analysis on ch2:
                case or ua: Γ ⊢ {M0/x0} m α0 M[x0] undefined is
                    _: Γ ⊢ {M0/x0} p α0 P1[x0] M[x0] undefined by rule No-Subst-P-App-2 on ua
                end case
            end case analysis
            suba: Γ ⊢ {M0/x0} m α0 M[x0] = M' by inversion on ch2
            _: Γ ⊢ {M0/x0} p α0 P1[x0] M[x0] = P1' M' by rule Subst-P-App on subf, suba
        end case
    end induction
end theorem
    
theorem subst-a-decidable:
    assumes Γ
    forall A[x0] assumes Γ, x0 : A0
    forall M0 assumes Γ
    forall α0
    exists Γ ⊢ {M0/x0} α0 A[x0] = A' or Γ ⊢ {M0/x0} α0 A[x0] undefined
    proof by induction on (A[x0] assumes Γ, x0 : A0):
        case P[x0] assumes Γ, x0 : A0 is
            ch: Γ ⊢ {M0/x0} p α0 P[x0] = P' or Γ ⊢ {M0/x0} p α0 P[x0] undefined by theorem subst-p-decidable on (P[x0] assumes Γ, x0:A0), M0, α0
            proof by case analysis on ch:
                case or sub: Γ ⊢ ({M0/x0} p α0 P[x0]) = P' is
                    _: Γ ⊢ {M0/x0} α0 P[x0] = P' by rule Subst-A-P on sub
                end case

                case or u: Γ ⊢ ({M0/x} p α0 P[x]) undefined is
                    _: Γ ⊢ {M0/x0} α0 P[x0] undefined by rule No-Subst-A-P on u 
                end case
            end case analysis
        end case

        case Π x : A1[x0] • A2[x0][x] assumes Γ, x0 : A0 is
            ch1: Γ ⊢ {M0/x0} α0 A1[x0] = A1' or Γ ⊢ {M0/x0} α0 A1[x0] undefined by induction hypothesis on (A1[x0] assumes Γ, x0:A0), M0, α0
            do case analysis on ch1:
                case or u1: Γ ⊢ {M0/x0} α0 A1[x0] undefined is
                    _: Γ ⊢ {M0/x0} α0 (Π x : A1[x0] • A2[x0][x]) undefined by rule No-Subst-A-Pi-1 on u1
                end case
            end case analysis
            subt: Γ ⊢ {M0/x0} α0 A1[x0] = A1' by inversion on ch1
            ch2: Γ, x:A1' ⊢ {M0/x0} α0 A2[x0][x] = A2'[x] or Γ, x:A1' ⊢ {M0/x0} α0 A2[x0][x] undefined by induction hypothesis on (A2[x0][x] assumes Γ, x:A1', x0:A0), M0, α0
            do case analysis on ch2:
                case or u2:  Γ, x:A1' ⊢ {M0/x0} α0 A2[x0][x] undefined is
                    _: Γ ⊢ {M0/x0} α0 (Π x : A1[x0] • A2[x0][x]) undefined by rule No-Subst-A-Pi-2 on subt, u2
                end case
            end case analysis
            subb: Γ, x:A1' ⊢ {M0/x0} α0 A2[x0][x] = A2'[x] by inversion on ch2
            _: Γ ⊢ {M0/x0} α0 (Π x : A1[x0] • A2[x0][x]) = (Π x : A1' • A2'[x]) by rule Subst-A-Pi on subt, subb
        end case

    end induction
end theorem

theorem subst-k-decidable:
    assumes Γ
    forall K[x0] assumes Γ, x0 : A0
    forall M0 assumes Γ
    forall α0
    exists Γ ⊢ {M0/x0} α0 K[x0] = K' or Γ ⊢ {M0/x0} α0 K[x0] undefined
    proof by induction on (K[x0] assumes Γ, x0 : A0):
        case type assumes Γ, x0 : A0 is
            _: Γ ⊢ {M0/x0} α0 type = type by rule Subst-K-Type
        end case

        case Π x : A1[x0] • K2[x0][x] assumes Γ, x0 : A0 is
            ch1: Γ ⊢ {M0/x0} α0 A1[x0] = A1' or Γ ⊢ {M0/x0} α0 A1[x0] undefined by theorem subst-a-decidable on (A1[x0] assumes Γ, x0:A0), M0, α0
            do case analysis on ch1:
                case or u1: Γ ⊢ {M0/x0} α0 A1[x0] undefined is
                    _: Γ ⊢ {M0/x0} α0 (Π x : A1[x0] • K2[x0][x]) undefined by rule No-Subst-K-Pi-1 on u1
                end case
            end case analysis
            subt: Γ ⊢ {M0/x0} α0 A1[x0] = A1' by inversion on ch1
            ch2: Γ, x:A1' ⊢ {M0/x0} α0 K2[x0][x] = K2'[x] or Γ, x:A1' ⊢ {M0/x0} α0 K2[x0][x] undefined by induction hypothesis on (K2[x0][x] assumes Γ, x:A1', x0:A0), M0, α0
            do case analysis on ch2:
                case or u2:  Γ, x:A1' ⊢ {M0/x0} α0 K2[x0][x] undefined is
                    _: Γ ⊢ {M0/x0} α0 (Π x : A1[x0] • K2[x0][x]) undefined by rule No-Subst-K-Pi-2 on subt, u2
                end case
            end case analysis
            subb: Γ, x:A1' ⊢ {M0/x0} α0 K2[x0][x] = K2'[x] by inversion on ch2
            _: Γ ⊢ {M0/x0} α0 (Π x : A1[x0] • K2[x0][x]) = (Π x : A1' • K2'[x]) by rule Subst-K-Pi on subt, subb
        end case

    end induction
end theorem


