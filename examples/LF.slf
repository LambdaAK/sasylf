// Lemma 2.10 (1) R
theorem subst-r-composeI:
    assumes Γ
    forall α0
    forall α2
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} r α2 R1[x0][x] = R[x0]
    forall s3: Γ, x:A ⊢ {M0/x0} r α0 R1[x0][x] = R1'[x]
    exists Γ ⊢ {M0/x0} r α0 R[x0] = R' 
    and    Γ ⊢ {M2'/x} r α2 R1'[x] = R'
    use induction on { α0, α2 }, s2
    sx: α2 ≤ α2 by rule sub-eq
    do case analysis on s3:
        case rule
            v3: R1'[x] : A2[x] ∈ (Γ, x : A)
            ------------------------- Subst-R-Var
            _: Γ, x : A ⊢ {M0/x0} r α0 R1'[x] = R1'[x]
            where R1[x0][x] := R1'[x]
        is
            proof by case analysis on v3:
                case rule
                    --------------- var
                    _: x : A ∈ (Γ, x : A)
                    where R1'[x] := x and A2[x] := A
                is
                    proof by contradiction on s2
                end case

                case rule
                    ------------------------------------ var
                    _: x' : A' ∈ (Γ', x' : A', x : A1[x'])
                    where A2[x] := A'
                is
                    // inversion didn't work to set R[x] := R'  (make goodXX.slf)
                    proof by case analysis on s2:
                        case rule
                            vx: R' : A3[x] ∈ (Γ, x : A0)
                            ------------------------- Subst-R-Var
                            _: (Γ, x : A0) ⊢ ({M2[x]/x'} r α2 R') = R'
                            where R[x] := R'
                        is
                            v': x' : A' ∈ (Γ', x':A') by rule var
                            v2: R0 : A' ∈ Γ by v'
                            s4: Γ ⊢ {M0/x0} r α0 R' = R' by rule Subst-R-Var on v2
                            s5: Γ ⊢ {M2'/x} r α2 R' = R' by rule Subst-R-Var on v2
                            proof by s4, s5
                        end case

                    end case analysis
                end case

            end case analysis
        end case
    end case analysis
    proof by case analysis on s2:
        case rule
            v: R[x0] : A1[x0] ∈ (Γ, x0 : A0)
            ------------------------- Subst-R-Var
            _: Γ, x0 : A0 ⊢ ({M2[x0]/x} r α2 R[x0]) = R[x0]
            where R1[x0][x] := R[x0]
        is
            proof by case analysis on v:
                case rule
                    ------------------------------------ var
                    _: x' : A' ∈ (Γ', x' : A', x0 : A3[x'])
                    where A1[x0] := A'
                is
                    proof by contradiction on s3
                end case

                case rule
                    -------------------------------- var
                    _: x0 : A0 ∈ (Γ, x0 : A0)
                    where A1[x0] := A0 and R[x0] := x0
                is
                    proof by contradiction on s3
                end case

            end case analysis
        end case

        case rule
            ------------------------- Subst-R-Const
            _: Γ, x0 : A0 ⊢ ({M2[x0]/x} r α2 c) = c
            where R1[x0][x] := c and R[x] := c
        is
            use inversion of rule Subst-R-Const on s3 where R1'[x] := c
            s4: Γ ⊢ {M0/x0} r α0 c = c by rule Subst-R-Const
            s5: Γ ⊢ {M2'/x} r α2 c = c by rule Subst-R-Const
            proof by s4, s5
        end case

        case rule
            s21: Γ, x0 : A0 ⊢ ({M2[x0]/x} r α2 R11[x0][x]) = R21[x0]
            s22: Γ, x0 : A0 ⊢ ({M2[x0]/x} m α2 M12[x0][x]) = M22[x0]
            ------------------------------------- Subst-R-App
            _:   Γ, x0 : A0 ⊢ ({M2[x0]/x} r α2 (R11[x0][x] M12[x0][x])) = (R21[x0] M22[x0])
            where R1[x0][x] := R11[x0][x] M12[x0][x] 
             and  R[x0] := R21[x0] M22[x0]
        is
            s31: Γ, x:A ⊢ {M0/x0} r α0 R11[x0][x] = R31[x] and
            s32: Γ, x:A ⊢ {M0/x0} m α0 M12[x0][x] = M32[x] by inversion on s3
            where R1'[x] := R31[x] M32[x]
            s41: Γ ⊢ {M0/x0} r α0 R21[x0] = R4 and 
            s51: Γ ⊢ {M2'/x} r α2 R31[x] = R4 by induction hypothesis on α0, α2, s1, s21, s31
            s42: Γ ⊢ {M0/x0} m α0 M22[x0] = M4 and
            s52: Γ ⊢ {M2'/x} m α2 M32[x] = M4 by theorem subst-m-composeI on α0, α2, sx, s1, s22, s32
            s4:  Γ ⊢ {M0/x0} r α0 R21[x0] M22[x0] = R4 M4 by rule Subst-R-App on s41, s42
            s5:  Γ ⊢ {M2'/x} r α2 R31[x] M32[x] = R4 M4 by rule Subst-R-App on s51, s52
            proof by s4, s5
        end case

    end case analysis
end theorem
