// Lemma 2.10 (2)
theorem subst-rh-r-compose:
    assumes Γ
    forall α0
    forall α2
    forall s1: Γ ⊢ {M0/x0} m α0 M2[x0] = M2'
    forall s2: Γ, x0:A0 ⊢ {M2[x0]/x} r α2 R1[x0][x] = M[x0] : α
    forall s3: Γ, x:A ⊢ {M0/x0} r α0 R1[x0][x] = R1'[x]
    exists Γ ⊢ {M0/x0} m α0 M[x0] = M' and Γ ⊢ {M2'/x} r α2 R1'[x] = M' : α
    use induction on { α0, α2 }, s2
    sx: α2 ≤ α2 by rule sub-eq
    do case analysis on s3:
        case rule
            v3: R1'[x] : A2[x] ∈ (Γ, x : A)
            ------------------------- Subst-R-Var
            _: Γ, x : A ⊢ {M0/x0} r α0 R1'[x] = R1'[x]
            where R1[x0][x] := R1'[x]
        is
            proof by case analysis on v3:
                case rule
                    --------------- var
                    _: x : A ∈ (Γ, x : A)
                    where R1'[x] := x and A2[x] := A
                is
                    use inversion of rule Subst-RH-Var on s2
                    where M[x0] := M2[x0] and α := α2
                    s5: Γ ⊢ {M2'/x} r α2 x = M2' : α2 by rule Subst-RH-Var
                    proof by s1, s5
                end case

                case rule
                    ------------------------------------ var
                    _: x' : A' ∈ (Γ', x' : A', x : A1[x'])
                    where A2[x] := A'
                is
                    proof by contradiction on s2
                end case

            end case analysis
        end case
    end case analysis
    proof by case analysis on s2:
        case rule
            ------------------------------------ Subst-RH-Var
            _: Γ, x0 : A0 ⊢ ({M2[x0]/x} r α2 x) = M2[x0] : α2
            where R1[x0][x] := x and M[x0] := M2[x0] and α := α2
        is
            proof by contradiction on s3
        end case

        case rule // Γ, x0:A0 ⊢ {M2[x0]/x} r α2 R1[x0][x] = M[x0] : α
            s21: Γ, x0:A0 ⊢ ({M2[x0]/x}  r α2 R11[x0][x]) = (λ x' • M11[x0][x']) : (α1 → α)
            s22: Γ, x0:A0 ⊢ ({M2[x0]/x}  m α2 M12[x0][x]) = M4[x0]
            s23: Γ, x0:A0 ⊢ ({M4[x0]/x'} m α1 M11[x0][x']) = M[x0]
            ----------------------------------------------------- Subst-RH-App
            _: Γ, x0:A0 ⊢ ({M2[x0]/x} r α2 (R11[x0][x] M12[x0][x])) = M[x0] : α
            where R1[x0][x] := R11[x0][x] M12[x0][x] 
        is
            s31: Γ, x:A ⊢ {M0/x0} r α0 R11[x0][x] = R31[x] and
            s32: Γ, x:A ⊢ {M0/x0} m α0 M12[x0][x] = M32[x] by inversion on s3
            where R1'[x] := R31[x] M32[x]
            s41: Γ ⊢ {M0/x0} m α0 (λ x' • M11[x0][x']) = M41 and
            s51: Γ ⊢ {M2'/x} r α2 R31[x] = M41 : α1 → α by induction hypothesis on α0, α2, s1, s21, s31
            s44: Γ, x' : 0 ⊢ ({M0/x0} m α0 M11[x0][x']) = M411[x'] by inversion of rule Subst-M-Lam on s41
            where M41 := λ x • M411[x] 
            s42: Γ ⊢ {M0/x0} m α0 M4[x0] = M51 and
            s52: Γ ⊢ {M2'/x} m α2 M32[x] = M51 by theorem subst-m-composeI on α0, α2, sx, s1, s22, s32
            sbx: α1 → α ≤ α2 by theorem head-substitution-size on s21
            sb11: α1 ≤ α1 by rule sub-eq
            sub11': α1 ≤ α1 → α by rule sub-arrow1 on sb11
            s4: Γ ⊢ {M0/x0}  m α0 M[x0] = M' and
            s53:Γ ⊢ {M51/x'} m α1 M411[x'] = M' by case analysis on sbx:
                case rule
                    ------------------------ sub-eq
                    _: (α1 → α) ≤ (α1 → α)
                    where α2 := α1 → α
                is
                    proof by theorem subst-m-composeI on α0, α1, sb11, s42, s23, s44
                end case

                case rule
                    sub13: (α1 → α) ≤ α3
                    ------------------- sub-arrow1
                    _: (α1 → α) ≤ (α3 → α4)
                    where α2 := α3 → α4
                is
                    sbx': α1 ≤ α3 by lemma subexpression-transitive on sub11', sub13
                    proof by theorem subst-m-composeI on α0, α3, sbx', s42, s23, s44
                end case

                case rule
                    sub14: (α1 → α) ≤ α4
                    ------------------- sub-arrow2
                    _: (α1 → α) ≤ (α3 → α4)
                    where α2 := α3 → α4
                is
                    sbx': α1 ≤ α4 by lemma subexpression-transitive on sub11', sub14
                    proof by theorem subst-m-composeI on α0, α4, sbx', s42, s23, s44
                end case
            end case analysis
            s5: Γ ⊢ {M2'/x} r α2 R31[x] M32[x] = M' : α by rule Subst-RH-App on s51, s52, s53
            proof by s4,s5
        end case

    end case analysis
end theorem   
