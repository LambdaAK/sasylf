lemma simple-type-unique:
    assumes Γ 
    forall sm1: Γ ⊢ (A)⁻ = α' 
    forall sm2: Γ ⊢ (A)⁻ = α''
    exists α' = α''
    proof by induction on sm1:
        case rule
            ------------------ simple-fam
            _: Γ ⊢ (a)⁻ = a
            where A := a and α' := a
        is
            use inversion on sm2 where α'' := a
            eq: α' = α'' by rule simple-eq
        end case

        case rule
            sm1': Γ ⊢ (P)⁻ = α'
            -------------------- simple-app
            _: Γ ⊢ (P M) ⁻= α'
            where A := (P M)
        is
            sm2': Γ ⊢ (P)⁻ = α'' by inversion on sm2
            proof by induction hypothesis on sm1', sm2'
        end case

        case rule
            sm11: Γ ⊢ (A1)⁻ = α1'
            sm12: Γ, x : A1 ⊢ (A2[x])⁻ = α2'
            ------------------------------------- simple-prod
            _: Γ ⊢ (Π x : A1 • A2[x])⁻ = (α1' → α2')
            where A := Π x : A1 • A2[x] and α' := α1' → α2'
        is
            sm21: Γ ⊢ (A1)⁻ = α1'' and
            sm22: Γ, x : A1 ⊢ (A2[x])⁻ = α2'' by inversion of rule simple-prod on sm2
            where α'' := α1'' → α2''
            eq1: α1' = α1'' by induction hypothesis on sm11, sm21
            eq2: α2' = α2'' by induction hypothesis on sm12, sm22
            use inversion on eq1 where α1' := α1''
            use inversion on eq2 where α2' := α2''
            proof by rule simple-eq
        end case
    end induction
end lemma

lemma simple-prod-inversion:
    assumes Γ
    forall sim12: Γ ⊢ (Π x:A1 • A2[x])⁻ = α12 
    forall sim1: Γ ⊢ (A1)⁻ = α1 
    exists α12 = α1 → α2  and Γ, x:A1 ⊢ (A2[x])⁻ = α2 
    proof by case analysis on sim12:
        case rule
            sim1': Γ ⊢ (A1)⁻ = α1'
            sim2': Γ, x : A1 ⊢ (A2[x])⁻ = α2
            ------------------------------------- simple-prod
            _: Γ ⊢ (Π x : A1 • A2[x])⁻ = (α1' → α2)
            where α12 := α1' → α2
        is
            eq: α1 = α1' by lemma simple-type-unique on sim1, sim1'
            use inversion on eq where α1' := α1
            eq12: α1 → α2 = α1 → α2 by rule simple-eq
            proof by eq12, sim2'
        end case
    end case analysis
end lemma


lemma subexpression-transitive:
    forall s12: α1 ≤ α2
    forall s23: α2 ≤ α3
    exists α1 ≤ α3
    proof by induction on s23:
        case rule
            ----------- sub-eq
            _: α2 ≤ α2
            where α3 := α2
        is
            proof by s12
        end case

        case rule
            s231: α2 ≤ α31
            ------------------- sub-arrow1
            _: α2 ≤ (α31 → α32)
            where α3 := (α31 → α32)
        is
            s131: α1 ≤ α31 by induction hypothesis on s12, s231
            proof by rule sub-arrow1 on s131
        end case

        case rule
            s232: α2 ≤ α32
            ------------------- sub-arrow2
            _: α2 ≤ (α31 → α32)
            where α3 := (α31 → α32)
        is
            s132: α1 ≤ α32 by induction hypothesis on s12, s232
            proof by rule sub-arrow2 on s132
        end case

    end induction
end lemma     
  
lemma subexpression-arrow-inversion:
    forall sbe: α11 → α12 ≤ α2
    exists α2 = α21 → α22 and (α11 ≤ α21 or α11 ≤ α22 ) and (α12 ≤ α21 or α12 ≤ α22)
    sb11: α11 ≤ α11 by rule sub-eq
    sb111: α11 ≤ α11 → α12 by rule sub-arrow1 on sb11
    sb12: α12 ≤ α12 by rule sub-eq
    sb121: α12 ≤ α11 → α12 by rule sub-arrow2 on sb12
    proof by case analysis on sbe:
        case rule
            --------------------------- sub-eq
            _: (α11 → α12) ≤ (α11 → α12)
            where α2 := α11 → α12
        is
            d1: (α11 → α12) = (α11 → α12) by rule simple-eq
            d2: α11 ≤ α11 by rule sub-eq
            d3: α12 ≤ α12 by rule sub-eq
            proof by d1, d2, d3
        end case

        case rule
            sb1: (α11 → α12) ≤ α21
            --------------------------- sub-arrow1
            _: (α11 → α12) ≤ (α21 → α22)
            where α2 := α21 → α22
        is
            d1: (α21 → α22) = (α21 → α22) by rule simple-eq
            d2: α11 ≤ α21 by lemma subexpression-transitive on sb111, sb1
            d3: α12 ≤ α21 by lemma subexpression-transitive on sb121, sb1
            proof by d1, d2, d3
        end case

        case rule
            sb2: (α11 → α12) ≤ α22
            ------------------- sub-arrow2
            _: (α11 → α12) ≤ (α21 → α22)
            where α2 := α21 → α22
        is
            d1: (α21 → α22) = (α21 → α22) by rule simple-eq
            d2: α11 ≤ α22 by lemma subexpression-transitive on sb111, sb2
            d3: α12 ≤ α22 by lemma subexpression-transitive on sb121, sb2
            proof by d1, d2, d3
        end case

    end case analysis
end lemma

/// lemma 2.2
theorem head-substitution-size:
    assumes Γ
    forall s: Γ ⊢ {M0/x0} r α0 R[x0] = M' : α
    exists α ≤ α0
    proof by induction on s:
        case rule
            ------------------------------------ Subst-RH-Var
            _: Γ ⊢ ({M0/x0} r α0 x0) = M0 : α0
            where R[x0] := x0 and M' := M0 and α := α0
        is
            proof by rule sub-eq
        end case

        case rule
            s': Γ ⊢ ({M0/x0} r α0 R1[x0]) = (λ x • M1[x]) : (α1 → α)
            _: Γ ⊢ ({M0/x0} m α0 M2[x0]) = M2'
            _: Γ ⊢ ({M2'/x} m α1 M1[x]) = M'
            ------------------------------------------- Subst-RH-App
            _: Γ ⊢ ({M0/x0} r α0 (R1[x0] M2[x0])) = M' : α
            where R[x0] := R1[x0] M2[x0]
        is
            sub1: α1 → α ≤ α0 by induction hypothesis on s'
            refl: α ≤ α by rule sub-eq
            step: α ≤ α1 → α by rule sub-arrow2 on refl
            proof by lemma subexpression-transitive on step, sub1
        end case

    end induction
end theorem


/// Lemma 2.3 (Uniqueness of Substitution and Synthesis)
// This has four parts

// first some helper lemmas


// Lemma 2.3 (1)
theorem subst-r-rh-incompatible:
    assumes Γ
    forall s1: Γ ⊢ σ = E'
    forall s2: Γ ⊢ σ = M2 : α'
    exists contradiction
    do case analysis on s1:
        case rule
            i: R1 : A0 ∈ Γ
            ------------------------- Subst-R-Var
            _: Γ ⊢ ({M0/x} r α0 R1) = R1
            where σ := {M0/x0} r α0 R1 and E' := R1
        is
            proof by case analysis on i:
                case rule
                    --------------- var
                    _: x : A0 ∈ (Γ', x : A0)
                is
                    proof by contradiction on s2
                end case
            end case analysis
        end case
    end case analysis
    proof by induction on s2:
        case rule
            ------------------------------------ Subst-RH-Var
            _: Γ ⊢ ({M0/x} r α0 x) = M0 : α0
            where σ := {M0/x0} r α0 x0 and M2 := M0 and α' := α0
        is
            proof by contradiction on s1
        end case

        case rule
            s2': Γ ⊢ {M0/x0}  r α0 R0[x0] = (λ x • M0'[x]) : α2 → α'
            _:   Γ ⊢ {M0/x0}  m α0 M1[x0] = M1''
            _:   Γ ⊢ {M1''/x} m α2 M0'[x] = M2
            ------------------------------------------- Subst-RH-App
            _: Γ ⊢ ({M0/x0} r α0 (R0[x0] M1[x0])) = M2 : α'
            where σ := {M0/x0} r α0 (R0[x0] M1[x0])
        is
            s1': Γ ⊢ ({M0/x0} r α0 R0[x0]) = R0' and
            _: Γ ⊢ ({M0/x0} m α0 M1[x0]) = M1' by inversion on s1
            where E' := R0' M1'
            proof by induction hypothesis on s1', s2'
        end case
    end induction
end theorem

// Lemma 2.3 (2)
theorem subst-unique:
    assumes Γ
    forall s1: Γ ⊢ σ = E'
    forall s2: Γ ⊢ σ = E''
    exists Γ ⊢ E' == E''
    do case analysis on s2:
        case rule
            i2: R : A2 ∈ Γ
            ------------------------- Subst-R-Var
            _: Γ ⊢ ({M0/x} r α0 R) = R
            where σ := {M0/x0} r α0 R and E'' := R
        is
            proof by case analysis on i2:
                case rule
                    --------------- var
                    _: x : A2 ∈ (Γ', x : A2)
                is
                    proof by case analysis on s1:
                        case rule
                            i1: R : A1 ∈ Γ
                            ------------------------- Subst-R-Var
                            _: Γ ⊢ ({M0/x} r α0 R) = R
                            where E' := R
                        is
                            proof by rule term-eq
                        end case
                    end case analysis
                end case
            end case analysis
        end case
    end case analysis
    
    proof by induction on s1:
        case rule
            ------------------------------ Subst-K-Type
            _: Γ ⊢ ({M0/x0} α0 type) = type
            where σ := {M0/x0} α0 type and E' := type
        is
            use inversion on s2 where E'' := type
            proof by rule term-eq
        end case
        case rule
            s11: Γ ⊢ ({M0/x} α0 A0[x]) = A1
            s12: Γ, x : A1 ⊢ ({M0/x0} α0 K0[x0][x]) = K'[x]
            ---------------------------------------------------------- Subst-K-Pi
            _: Γ ⊢ ({M0/x0} α0 (Π x : A0[x0] • K0[x0][x])) = (Π x : A1 • K'[x])
            where σ := {M0/x0} α0 (Π x : A0[x0] • K0[x0][x]) and E' := Π x : A1 • K'[x]
        is
            s21: Γ ⊢ ({M0/x} α0 A0[x]) = A2 and
            s22: Γ, x : A2 ⊢ ({M0/x'} α0 K0[x'][x]) = K''[x] by inversion on s2
            where E'' := Π x : A2 • K''[x]
            eqA: Γ ⊢ A1 == A2 by induction hypothesis on s11, s21
            use inversion on eqA where A2 := A1
            eqK: Γ, x:A1 ⊢ K'[x] == K''[x] by induction hypothesis on s12, s22
            use inversion on eqK where K''[x] := K'[x]
            proof by rule term-eq
        end case
        
        case rule
            s11: Γ ⊢ {M0/x0} p α0 P[x0] = P'
            ------------------------------ Subst-A-P
            _:   Γ ⊢ {M0/x0}   α0 P[x0] = P'
            where σ := {M0/x0} α0 P[x0] and E' := P'
        is
            s21: Γ ⊢ {M0/x0} p α0 P[x0] = P'' by inversion on s2 where E'' := P''
            proof by induction hypothesis on s11, s21
        end case

        case rule
            s11: Γ ⊢ ({M0/x0} α0 A1[x0]) = A1'
            s12: Γ, x : A1' ⊢ ({M0/x0} α0 A2[x0][x]) = A2'[x]
            --------------------------------------------------------------- Subst-A-Pi
            _: Γ ⊢ ({M0/x0} α0 (Π x : A1[x0] • A2[x0][x])) = (Π x : A1' • A2'[x])
            where σ := {M0/x0} α0 (Π x : A1[x0] • A2[x0][x]) and E' := Π x : A1' • A2'[x]
        is
            s21: Γ ⊢ {M0/x0} α0 A1[x0] = A1'' and
            s22: Γ, x : A1'' ⊢ {M0/x0} α0 A2[x0][x] = A2''[x] by inversion on s2
            where E'' := Π x : A1'' • A2''[x]
            eqA: Γ ⊢ A1' == A1'' by induction hypothesis on s11, s21
            use inversion on eqA where A1'' := A1'
            eqB: Γ, x : A1' ⊢ A2'[x] == A2''[x] by induction hypothesis on s12, s22
            use inversion on eqB where A2''[x] := A2'[x]
            proof by rule term-eq
        end case
        
        case rule
            ------------------------ Subst-P-Fam
            _: Γ ⊢ ({M0/x0} p α0 a) = a
            where σ := {M0/x0} p α0 a and E' := a
        is
            use inversion on s2 where E'' := a
            proof by rule term-eq
        end case

        case rule
            s11: Γ ⊢ ({M0/x} p α0 P1[x]) = P1'
            s12: Γ ⊢ ({M0/x} m α0 M1[x]) = M1'
            ------------------------------------- Subst-P-App
            _: Γ ⊢ ({M0/x} p α0 (P1[x] M1[x])) = (P1' M1')
            where σ := {M0/x0} p α0 (P1[x0] M1[x0]) and E' := P1' M1'
        is
            s21: Γ ⊢ ({M0/x} p α0 P1[x]) = P1'' and
            s22: Γ ⊢ ({M0/x} m α0 M1[x]) = M1'' by inversion on s2
            where E'' := P1'' M1''
            eqF: Γ ⊢ P1' == P1'' by induction hypothesis on s11, s21
            eqA: Γ ⊢ M1' == M1'' by induction hypothesis on s12, s22 
            use inversion on eqF where P1'' := P1'
            use inversion on eqA where M1'' := M1'
            proof by rule term-eq
        end case
        
        case rule
            s11: Γ ⊢ ({M0/x0} r α0 R[x0]) = R'
            ------------------------------- Subst-M-R
            _:   Γ ⊢ ({M0/x0} m α0 R[x0]) = R'
            where σ := {M0/x0} m α0 R[x0] and E' := R'
        is
            proof by case analysis on s2:
                case rule
                    s21: Γ ⊢ ({M0/x} r α0 R[x]) = R''
                    ------------------------------- Subst-M-R
                    _:   Γ ⊢ ({M0/x} m α0 R[x]) = R''
                    where E'' := R''
                is
                    proof by induction hypothesis on s11, s21
                end case

                case rule
                    s21: Γ ⊢ ({M0/x} r α0 R[x]) = M'' : α''
                    ------------------------------------ Subst-M-RH
                    _: Γ ⊢ ({M0/x} m α0 R[x]) = M''
                    where E'' := M''
                is
                    _: contradiction by theorem subst-r-rh-incompatible on s11, s21
                end case

            end case analysis
        end case

        case rule
            s11: Γ ⊢ ({M0/x0} r α0 R[x0]) = M' : α'
            ------------------------------------ Subst-M-RH
            _:   Γ ⊢ ({M0/x0} m α0 R[x0]) = M'
            where σ := {M0/x0} m α0 R[x0] and E' := M'
        is
            proof by case analysis on s2:
                case rule
                    s21: Γ ⊢ ({M0/x} r α0 R[x]) = R''
                    ------------------------------- Subst-M-R
                    _:   Γ ⊢ ({M0/x} m α0 R[x]) = R''
                    where E'' := R''
                is
                    _: contradiction by theorem subst-r-rh-incompatible on s21, s11
                end case

                case rule
                    s21: Γ ⊢ ({M0/x} r α0 R[x]) = M'' : α''
                    ------------------------------------ Subst-M-RH
                    _:   Γ ⊢ ({M0/x} m α0 R[x]) = M''
                    where E'' := M''
                is
                    eqM: Γ ⊢ M' == M'' and
                    eqST: α' = α'' by theorem subst-h-unique on s11, s21
                    proof by eqM
                end case

            end case analysis
        end case

        case rule
            s11: Γ, x : 0 ⊢ ({M0/x0} m α0 M1[x0][x]) = M1'[x]
            -------------------------------------------------- Subst-M-Lam
            _:   Γ ⊢ ({M0/x0} m α0 (λ x • M1[x0][x])) = (λ x • M1'[x])
            where σ := {M0/x0} m α0 (λ x • M1[x0][x]) and E' := λ x • M1'[x]
        is
            s21: Γ, x : 0 ⊢ ({M0/x0} m α0 M1[x0][x]) = M1''[x] by inversion on s2
            where E'' := λ x • M1''[x]
            eqL: Γ, x:0 ⊢ M1'[x] == M1''[x] by induction hypothesis on s11, s21
            use inversion on eqL where M1''[x] := M1'[x]
            proof by rule term-eq
        end case

        case rule
            v1: R : A1 ∈ Γ
            ------------------------- Subst-R-Var
            _: Γ ⊢ ({M0/x} r α0 R) = R
            where σ := {M0/x0} r α0 R and E' := R
        is
            proof by case analysis on v1:
                case rule
                    --------------- var
                    _: x : A1 ∈ (Γ', x : A1)
                is
                    proof by contradiction on s2
                end case
            end case analysis
        end case

        case rule
            ------------------------- Subst-R-Const
            _: Γ ⊢ ({M0/x} r α0 c) = c
            where σ := {M0/x0} r α0 c and E' := c
        is
            use inversion on s2 where E'' := c
            proof by rule term-eq
        end case

        case rule
            f1: Γ ⊢ ({M0/x0} r α0 R0[x0]) = R0'
            a1: Γ ⊢ ({M0/x0} m α0 M1[x0]) = M1'
            ------------------------------------- Subst-R-App
            _:  Γ ⊢ ({M0/x0} r α0 (R0[x0] M1[x0])) = (R0' M1')
            where σ := {M0/x0} r α0 (R0[x0] M1[x0]) and E' := R0' M1'
        is
            f2: Γ ⊢ ({M0/x0} r α0 R0[x0]) = R0'' and
            a2: Γ ⊢ ({M0/x0} m α0 M1[x0]) = M1'' by inversion on s2
            where E'' := R0'' M1''
            e1: Γ ⊢ R0' == R0'' by induction hypothesis on f1, f2
            e2: Γ ⊢ M1' == M1'' by induction hypothesis on a1, a2
            use inversion on e1 where R0'' := R0'
            use inversion on e2 where M1'' := M1'
            proof by rule term-eq
        end case
    end induction
end theorem
