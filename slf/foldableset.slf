package slf

module foldableset

requires

  abstract syntax e

  abstract judgment combine: e + e = e

provides

  terminals empty

  syntax s ::= empty | e :: s

  judgment foldset: s -> e

  ----------- fold-1 // singleton list evaluates to the element
  e :: empty -> e


  s -> e2
  e1 + e2 = e3
  ----------- fold-2 // combine the head with the result of folding the tail
  e1 :: s -> e3

