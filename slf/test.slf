package slf

module test

provides


terminals T F

syntax b ::= T | F

judgment bleq: b <= b

  ---- bleq-1
  T <= T

  ---- bleq-2
  F <= F

  ---- bleq-3
  F <= T

theorem total-bleq:
  forall b1
  forall b2
  exists b1 <= b2 or b2 <= b1
  proof by unproved
end theorem

module boolpo = slf.po[b, bleq, total-bleq]